{
    "name": "@alcs/react-native-input-autofocus",
    "version": "0.0.1",
    "description": "Support for next input focusing functionality",
    "main": "./dist/commonjs/index.js",
    "module": "./dist/module/index.js",
    "react-native": "./src/index.tsx",
    "types": "./dist/typescript/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/fracht/react-native-input-autofocus.git",
        "directory": "lib"
    },
    "exports": {
        "types": "./dist/typescript/index.d.ts",
        "import": "./dist/module/index.js",
        "require": "./dist/commonjs/index.js"
    },
    "files": [
        "dist/",
        "src/"
    ],
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "react-native",
        "autofocus",
        "next-field",
        "enter",
        "onenter",
        "input",
        "textField"
    ],
    "author": "Aleksandras Bogdanovas",
    "license": "MIT",
    "scripts": {
        "build": "bob build",
        "test": "jest",
        "prepublishOnly": "rimraf ./prepared-package && pnpm build && clean-publish && publint ./prepared-package"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.19.0",
        "@babel/runtime": "^7.12.5",
        "@sirse-dev/eslint-config": "^1.0.12",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.16",
        "@types/react": "^18.0.18",
        "@types/react-native": "^0.69.6",
        "clean-publish": "^4.2.0",
        "eslint": "^8.23.0",
        "jest": "^29.0.2",
        "prettier": "^2.7.1",
        "publint": "^0.1.12",
        "react": "18.1.0",
        "react-native": "0.70.0",
        "react-native-builder-bob": "^0.18.3",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "react-native-builder-bob": {
        "source": "./src",
        "output": "dist",
        "targets": [
            [
                "commonjs",
                {
                    "copyFlow": true
                }
            ],
            "module",
            "typescript"
        ]
    },
    "peerDependencies": {
        "react": "^18.1.0",
        "react-native": "^0.70.0 || ^0.71.0"
    },
    "dependencies": {
        "@sirse-dev/safe-context": "^0.3.0"
    }
}
